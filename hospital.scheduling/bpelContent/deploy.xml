<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:patient="http://hospital/patient/" xmlns:agenda="http://hospital/agenda/" xmlns:doctor="http://hospital/doctor/" xmlns:room="http://hospital/room/" xmlns:scheduling="http://hospital/scheduling/">
  <process name="scheduling:scheduling">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="link_scheduler">
      <service name="scheduling:SchedulingService" port="scheduling"/>
    </provide>
    <provide partnerLink="link_agenda">
      <service name="agenda:AgendaCallbackService" port="agendaCallback"/>
    </provide>
    <invoke partnerLink="link_room">
      <service name="room:RoomService" port="room"/>
    </invoke>
    <invoke partnerLink="link_doctor">
      <service name="doctor:DoctorService" port="doctor"/>
    </invoke>
    <invoke partnerLink="link_agenda">
      <service name="agenda:AgendaService" port="agenda"/>
    </invoke>
    <invoke partnerLink="link_patient">
      <service name="patient:PatientService" port="patient"/>
    </invoke>
  </process>
</deploy>